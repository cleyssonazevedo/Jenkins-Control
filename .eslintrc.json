{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "new-cap": "off",
        "no-unused-vars": "warn",
        "indent": [
            "warn",
            4
        ],
        "object-curly-spacing": [
            2,
            "always"
        ]
    }
}